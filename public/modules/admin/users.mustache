<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="page-header">
            <h1>users</h1>
        </div>
        <table class="table table-hover">
            <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
                <th>Sign Up Date</th>
            </tr>
            </thead>
            <tbody>
            {{#users}}
                <tr>
                    <td>{{attributes.username}}</td>
                    <td>{{attributes.email}}</td>
                    <td>{{createdAt}}</td>
                </tr>
            {{/users}}
            </tbody>
        </table>
    </div>
</div>